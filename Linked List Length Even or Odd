class GFG
{
	int isLengthEvenorOdd(Node head1)
	{
	    int count=0;
	    if(head1==null)
	    {
	        return 0;
	    }
	    Node temp =head1;
	    while(temp!=null)
	    {
	       temp=temp.next;
	       count++;
	    }
	    if(count%2==0)
	    {
	        return 0;
	    }
	    else{
	        return 1;
	        
	    }
	    
	}
}
